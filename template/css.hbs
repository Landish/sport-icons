{{#each layouts}}
  {{#each sprites}}
.{{cssesc ../classname}} {
    background-image: url('{{escimage url}}');
    display: inline-block;
    vertical-align: middle;
    height: 16px;
    width: 16px;
}
{{/each}}
{{#each layout.items}}
.{{../classname}}.{{meta.name}} {
    background-position: -{{baseDim x}}px -{{baseDim y}}px;
}
{{/each}}
{{/each}}